{
  "name": "tasky-vue",
  "version": "0.1.0",
  "private": true,
  "main": "src/background.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:win32": "electron-packager . App --platform=win32 --arch=x64 --icon=icon.ico --out=./build_win32 --asar --app-version=1.0.0",
    "build:linux": "electron-packager . App --platform=linux --arch=x64 --icon=icon.ico --out=./build_linux --asar --app-version=1.0.0 --overwrite",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@xmldom/xmldom": "^0.8.11",
    "ant-design-vue": "^4.2.6",
    "core-js": "^3.8.3",
    "echarts": "^5.6.0",
    "electron": "^40.0.0",
    "element-plus": "^2.11.5",
    "path-browserify": "^1.0.1",
    "tdesign-vue-next": "^1.14.2",
    "vue-router": "^4.5.1",
    "xml2js": "^0.6.2",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.12.16",
